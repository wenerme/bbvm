root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = tab

[**.java]
indent_style = space
indent_size = 4

[**.groovy]
indent_style = space
indent_size = 4

[**.gradle]
indent_style = space
indent_size = 4

[**.g4]
indent_style = space
indent_size = 4


[**.go]
indent_style = tab
indent_size = 2

[**.md]
indent_style = space
indent_size = 2
